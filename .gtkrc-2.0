# -- GTK THEME OPTIONS

gtk-theme-name = "spacedust"
gtk-icon-theme-name = "gperfection2"
gtk-font-name = "Sans 8"
gtk-toolbar-icon-size = GTK_ICON_SIZE_SMALL_TOOLBAR

style "gvimfix" {
  font_name = "Profont 8"
  bg[NORMAL] = "#131313"
}
widget "vim-main-window.*GtkForm" style "gvimfix"
widget "vim-main-window.*GtkLabel" style "gvimfix"
