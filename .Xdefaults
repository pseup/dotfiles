! GENERAL {{{

Xft*dpi: 96
Xft*hinting: true
Xft*hintstyle: hintsfull
Xft*antialias: true
Xft*rgba: rgb

Xcursor*theme: Vanilla-DMZ
Xcursor*size: 16

! }}}
! COLORS {{{

!Black
*color0:  #454545
*color8:  #222222
!Red
*color1:  #9E5641
*color9:  #CC896D
!Green
*color2:  #C2D797
*color10: #7DB37D
!Yellow
*color3:  #EFC666
*color11: #FFBD73
!Blue
*color4:  #94C7D7
*color12: #869AB1
!Magenta
*color5:  #925372
*color13: #E165A1
!Cyan
*color6:  #4c8ea1
*color14: #6bc1d0
!White
*color7:  #d2d2d2
*color15: #ffffff

! }}}
! URXVT {{{

URxvt*font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*

URxvt*depth: 32
URxvt*background: [96]#131313
URxvt*foreground: #A9A9A9

URxvt*title:      >
URxvt*geometry:   84x24
URxvt*saveLines:  10000
URxvt*scrollBar:  false
URxvt*visualBell: false
URxvt*urgentOnBell: true

!URxvt*tabbed.tabbar-bg: -1
!URxvt*tabbed.tabbar-fg: 0
!URxvt*tabbed.tab-bg: 0
!URxvt*tabbed.tab-fg: 12

popTerm*background: [96]#212121
popTerm*geometry: 84x8

! }}}
